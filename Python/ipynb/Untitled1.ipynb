   "source": [
    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "\n",
    "def myClick():\n",
    "    label = Label(root, text=\"You clicked me!!\")\n",
    "    label.pack()\n",
    "\n",
    "myButton = Button(root, text=\"Click me!\", command=myClick, bg=\"blue\", fg=\"white\")\n",
    "myButton.pack()\n",
    "\n",
    "root.mainloop()"
   ]
