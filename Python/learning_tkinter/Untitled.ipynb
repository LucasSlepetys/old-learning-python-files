    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "\n",
    "def callback(event):\n",
    "    print (\"clicked at\", event.x, event.y)\n",
    "\n",
    "frame = Frame(root, width=550, height=520)\n",
    "#(event, handler)\n",
    "frame.bind(\"<Button-1>\", callback)\n",
    "frame.pack()\n",
    "\n",
    "root.mainloop()"
   ]
